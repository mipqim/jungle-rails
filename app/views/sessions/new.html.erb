<section class="session-new">

  <header class="page-header">
    <h1>Login</h1>
  </header>

  <div class="well">
    <%= form_tag '/login' do %>
      <p>Email: <%= text_field_tag :email %></p>
      <p>Password: <%= password_field_tag :password %></p>
      <p><%= submit_tag "Submit" %></p>  
    <% end %>
  </div>
</section>
